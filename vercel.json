{
    "name": "ts-index",
    "version": 2,
    "cleanUrls": true,
    "redirects": [
        { "source": "ts3.ink", "destination": "https://ts3.gs" }
    ],
    "headers": [
        {
          "source": "/(.*)",
          "headers" : [
            {
              "key" : "X-Content-Type-Options",
              "value" : "nosniff"
            },
            {
              "key" : "X-Frame-Options",
              "value" : "DENY"
            },
            {
              "key" : "X-XSS-Protection",
              "value" : "1; mode=block"
            }
          ]
        }
    ]
}